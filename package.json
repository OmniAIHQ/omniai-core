{
  "name": "@omniaihq/core",
  "version": "0.1.0",
  "description": "Provider-agnostic AI SDK foundation for OmniAI",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "husky",
    "commit": "cz"
  },
  "keywords": [
    "ai",
    "sdk",
    "provider-agnostic",
    "typescript"
  ],
  "author": "OmniAIHQ",
  "license": "MIT",
  "sideEffects": false,
  "devDependencies": {
    "@types/node": "^18.0.0",
    "eslint": "^9.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0",
    "typescript-eslint": "^8.0.0",
    "husky": "^9.0.0",
    "@commitlint/cli": "^19.0.0",
    "@commitlint/config-conventional": "^19.0.0",
    "commitizen": "^4.0.0",
    "cz-conventional-changelog": "^3.0.0"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "engines": {
    "node": ">=18"
  }
}
